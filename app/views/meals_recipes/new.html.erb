<h1>New meals_recipe</h1>

<% form_for(@meals_recipe) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :meal_id %><br />
    <%= f.text_field :meal_id %>
  </p>
  <p>
    <%= f.label :recipe_id %><br />
    <%= f.text_field :recipe_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', meals_recipes_path %>
